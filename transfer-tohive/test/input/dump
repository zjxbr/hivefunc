<script style="JavaScript">
<!--
        
var _previousValue;
        
function savePreviousValue ( elementObject ) {
  _previousValue = elementObject.value;
}

function restorePreviousValue( elementObject ) {
  elementObject.value = _previousValue;
}

var _changedObject;

function setChangedObject( elementObject ) {
  _changedObject = elementObject;
}

function getChangedObject( elementObject ) {
  return _changedObject;
}
      
function focusAndSelectChangedObject() {
  var elementObject = getChangedObject();
  elementObject.focus();
  elementObject.select();
}
      
function abortChange( elementObject ) {
  restorePreviousValue( elementObject );
  setChangedObject( elementObject );
  setTimeout( "focusAndSelectChangedObject()", 0);
  return false;
}

function checkValue ( valueObject ) {
  valueObject.value = valueObject.value.replace( /＼s+/g , "" );

  if ( isNaN( valueObject.value) ) {
    alert("You entered an unacceptable value. Please enter a numeric value.");
    return abortChange( valueObject );
  }
  var valueNum = new Number( valueObject.value );
  if ( valueNum < 0 ) {
    alert("You entered an value that is too low. Please enter a value that is greater than zero.")
    return abortChange( valueObject );
  }

  valueObject.value = roundOff( valueNum, 2 );

  return true;
}

function roundOff( val, p ) {
        
  val = "" + val;
  p = parseInt( p );

  var whole = "" + Math.round( val * Math.pow( 10, p ) );      
  var decPoint = whole.length - p;

  while ( decPoint <= 0 ) {

    whole = "0" + whole;
    decPoint = whole.length - p;
        
  }

  var result;

  result = whole.substring( 0, decPoint );
  result += ".";
  result += whole.substring( decPoint, whole.length );

  return result;

}

function showRates() {
    var expanded_rates = document.getElementById("standardRates_expanded");
    var collapsed_rates = document.getElementById("standardRates_collapsed");
    collapsed_rates.style.display = "none";
    expanded_rates.style.display = ""; //"table-row-group";
}
function hideRates() {
    var expanded_rates = document.getElementById("standardRates_expanded");
    var collapsed_rates = document.getElementById("standardRates_collapsed");
    collapsed_rates.style.display = ""; //"table-row-group";
    expanded_rates.style.display = "none";
}

function showBMVDRates() {
    var expanded_rates = document.getElementById("mediaRates_expanded");
    var collapsed_rates = document.getElementById("mediaRates_collapsed");
    collapsed_rates.style.display = "none";
    expanded_rates.style.display = ""; //"table-row-group";
}
function hideBMVDRates() {
    var expanded_rates = document.getElementById("mediaRates_expanded");
    var collapsed_rates = document.getElementById("mediaRates_collapsed");
    collapsed_rates.style.display = ""; //"table-row-group";
    expanded_rates.style.display = "none";
}

-->
</script>




