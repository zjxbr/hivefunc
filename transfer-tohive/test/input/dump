    if (typeof uet == 'function') {
      uet('bb', 'iss-init-pc', {wb: 1});
    }

    if (!window.$SearchJS && window.$Nav) {
      window.$SearchJS = $Nav.make('sx');
    }

  
  var opts = {
      host: "completion.amazon.co.jp/search/complete"
    , marketId: "6"
    , searchAliases: ["apparel", "aps", "automotive", "appliances", "baby", "baby-jp", "beauty", "blu-ray", "classical", "classical", "classical-domestic-sub", "computers", "digital-music", "digital-music-album", "digital-music-track", "digital-text", "diy", "dvd", "dvd-actor", "dvd-used", "electronics", "electronics-used", "english-books", "english-books-publ", "fashion", "fe-beauty-jp", "fe-books-jp", "fe-ce-jp", "fe-hpc-jp", "fe-music-jp", "food-beverage", "hobbies", "hobby", "hpc", "instant-video", "jewelry", "jp-books-tree", "jp-classical-tree", "jp-dvd-tree", "jp-music-tree", "kitchen", "mobile-apps", "music-album", "music-artist", "music-domestic", "music-singles", "mi", "office-products", "pets", "popular", "shoes", "software", "sporting", "stripbooks", "stripbooks-music", "stripbooks-publ", "stripbooks-publishers", "toys", "toys-used", "umd", "vg-gameboy", "vg-gameboy-advance", "vg-gamecube", "vg-nintendo-ds", "vg-playstation", "vg-playstation-portable", "vg-playstation2", "vg-playstation3", "vg-xbox", "vg-xbox360", "vhs", "videogames", "watch", "financial", "tradein-aps", "gift-cards", "warehouse-deals"]
