<script type="text/javascript">
  if (AAG == null || typeof(AAG) != "object") { var AAG = new Object();}
  
  AAG.getFeedbackPage = function (pageNum) {
    var feedbackUrl = "/gp/aag/ajax/paginatedFeedback.html?seller=A00331621I1OBH7XAX5VY&ref_=aag_m_ss&&currentPage=" + pageNum; 
    jQuery("#feedbackLoadingItem").nextAll().fadeTo("fast", 0.1, function() {
      jQuery("#feedbackLoadingItem").show();
    });
    jQuery.ajax({
      url: feedbackUrl,
      success: function(html) {
        jQuery("#feedbackLoadingItem").nextAll().remove();
        jQuery("#feedbackList").append(html).fadeTo("fast", 1.0, function() {
          jQuery("#feedbackLoadingItem").hide();
          jQuery("#feedbackLoadingItem").css("position", "absolute");
        });
      },
      error: function () {
        jQuery("#feedbackLoadingItem").nextAll().remove();
        jQuery("#feedbackList").append("<li>申し訳ありませんが、エラーが発生しました。</li>").fadeTo("fast", 1.0, function() {
          jQuery("#feedbackLoadingItem").hide();
        });
      }
    });
  }

  amznJQ.onReady('jQuery', function() {
    AAG.getFeedbackPage(0);
  });  
</script>


    
    




    
    
    
    





    

























































    
    
    
    
    
















    
    



    
    








    
    
    
    
    
    
    
    
    
    
